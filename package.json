{
  "name": "markboost-seo",
  "version": "0.1.0",
  "description": "A Node.js library to scan Markdown files, extract metadata, and generate SEO information (titles, descriptions) for English content using local LLMs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "test": "npx tsx src/test.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "seo",
    "markdown",
    "llm",
    "metadata",
    "static site generator",
    "content processing",
    "typescript",
    "nodejs",
    "transformers.js"
  ],
  "author": "Jeseong Lee (jeseong77)",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeseong77/markboost-seo.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "glob": "^11.0.2",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.14.9",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
